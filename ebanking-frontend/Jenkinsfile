pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'master',
                    url: 'https://github.com/oussamasebai593/Projet-DigitalBanking-SpringBoot-Angular.git'
            }
        }
    }

    post {
        success {
            echo '✅ Checkout réussi'
        }
        failure {
            echo '❌ Erreur lors du checkout'
        }
    }
}
